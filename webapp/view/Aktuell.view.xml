			<m:Button icon="sap-icon://action-settings" press="handleSettings" tooltip="{i18n>settings}"/>
			<m:ToggleButton icon="sap-icon://full-screen" press="handleFullscreen" class="sapUiSmallMarginBottom" tooltip="{i18n>fullscreen}"/>
			<m:Image decorative="false" densityAware="false" press="handleImage3Press" src="images/sonne.png" width="3em">
				<m:layoutData>
					<m:FlexItemData growFactor="1"/>
				</m:layoutData>
			</m:Image>
		</m:headerContent>
		<m:content>
			<m:Panel expandable="true" expanded="true"
				headerText="{i18n>pveinspeiseleistung} - {i18n>pvzaehler} {pvaktuell>/AktuellSet/zaehlernummer} - {i18n>pvmessung} {pvaktuell>/AktuellSet/letztemessung}"
